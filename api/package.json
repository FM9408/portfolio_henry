  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "start nodemon -L",
    "start": "node index.js",
    "restart": "npm restart",
    "build": "docker build . --tag gcr.io/fm9408portfolio/henry-protfolio-api",
    "auth": "gcloud auth configure docker",
