    "deploy": "npm run push && gcloud run deploy portfolio-henry-api --image gcr.io/fm9408portfolio/henry-protfolio-api --allow-unauthenticated --add-cloudsql-instances=fm9408portfolio:us-east1:henry-portfolio-api --memory 512Mi --cpu 1 --port 8080"
